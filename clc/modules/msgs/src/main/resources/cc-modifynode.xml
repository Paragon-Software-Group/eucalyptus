<binding force-classes="true" add-constructors="true">

	<mapping name="ModifyNode" class="edu.ucsb.eucalyptus.msgs.ModifyNode"
		extends="edu.ucsb.eucalyptus.msgs.EucalyptusMessage">
		<structure map-as="edu.ucsb.eucalyptus.msgs.EucalyptusMessage" />
		<value name="stateName" field="stateName" type="java.lang.String" />
	        <value name="nodeName" field="nodeName" type="java.lang.String" />	
	</mapping>

	<mapping name="ModifyNodeResponse"
		class="edu.ucsb.eucalyptus.msgs.ModifyNodeResponse" extends="edu.ucsb.eucalyptus.msgs.EucalyptusMessage">
		<structure map-as="edu.ucsb.eucalyptus.msgs.EucalyptusMessage" />
	</mapping>

</binding>
